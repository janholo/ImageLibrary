{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "RollingFile", "Args": { "pathFormat": "logs/log-{Date}.txt" } },
      { "Name": "Console"}
    ]
  },
  "AllowedHosts": "*",
  "RootFileSystemPath": "/data",
  "RelativePreviewPath": "preview"
}